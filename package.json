    "style-loader": "^0.13.1",
    "unescape-js": "^1.1.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-visualizer-plugin": "^0.1.5"
  },
  "dependencies": {
    "babel-polyfill": "^6.8.0",
    "file-dialog": "^0.0.7",
    "file-saver": "^1.3.8",
    "immutability-helper": "^2.6.4",
    "immutable": "^3.8.2",
    "jquery": "^3.3.1",
    "memoize-weak": "^1.0.2",
    "papaparse": "^4.6.0",
