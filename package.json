    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "15.4.2",
    "react-moment": "^0.6.5",
    "react-router": "^2.5.2",
    "react-style-proptype": "^3.2.1",
    "react-tap-event-plugin": "^2.0.1",
    "rx": "^4.1.0",
