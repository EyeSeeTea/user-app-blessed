    "karma-webpack": "1.7.0",
    "lodash.isequal": "^4.5.0",
    "lodash.isfinite": "^3.3.1",
    "lodash.isnumber": "^3.0.3",
    "loglevel": "^1.4.0",
    "material-ui": "^0.17",
    "mocha": "2.2.5",
    "moment": "^2.19.1",
    "node-sass": "^3.7.0",
    "phantomjs": "1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "precommit-hook": "3.0.0",
    "prettier": "^1.18.2",
